{
  "name": "react-twitter-auth-light",
  "version": "0.2.0",
  "description": "A Twitter logging component based on callbacks. You can manage everything.",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack src/index.js dist/index.js",
    "build:prod": "cross-env NODE_ENV=production webpack src/index.js dist/index.js",
    "lint": "eslint src/ --fix",
    "clean": "rm -rf node_modules && rm -rf bin",
    "test": "jest --config jest.config.json --coverage",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w222w/react-twitter-auth.git"
  },
  "keywords": [
    "React",
    "Twitter",
    "Login",
    "Auth",
    "oauth",
    "component"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w222w/react-twitter-auth/issues"
  },
  "peerDependencies": {
    "prop-types": "> 16.8.0 < 17",
    "react": "> 16.8.0 < 17",
    "react-dom": "> 16.8.0 < 17"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^21.1.0",
    "react": "^16.8.6",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.6",
    "webpack": "3.5.5"
  },
  "dependencies": {
    "url-search-params-polyfill": "^2.0.0",
    "whatwg-fetch": "2.0.3"
  }
